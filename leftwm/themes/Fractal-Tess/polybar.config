[module/mpd]
type = internal/mpd
format-online = <label-song> <icon-prev> <toggle> <icon-next>
label-song = %title%
icon-prev = ï«¿
icon-play = ï¤‰
icon-pause = ï£¥
icon-next = ï¬€
